{
  "name": "effect-template",
  "version": "1.0.0",
  "description": "Minimal Vite-powered TypeScript console starter using Effect",
  "main": "dist/main.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "changeset": "changeset",
    "changeset-publish": "changeset publish",
    "changeset-version": "changeset version",
    "lint": "npx @ton-ai-core/vibecode-linter src/",
    "check": "pnpm run typecheck",
    "prestart": "pnpm run build",
    "start": "node dist/main.js",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProverCoderAI/effect-template.git"
  },
  "keywords": [
    "effect",
    "typescript",
    "vite",
    "console"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/ProverCoderAI/effect-template/issues"
  },
  "homepage": "https://github.com/ProverCoderAI/effect-template#readme",
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "patchedDependencies": {
      "@changesets/get-github-info@0.6.0": "patches/@changesets__get-github-info@0.6.0.patch"
    },
    "ignoredBuiltDependencies": [
      "esbuild",
      "node-pty"
    ]
  },
  "dependencies": {
    "@effect/schema": "^0.75.5",
    "effect": "^3.19.8",
    "ts-morph": "^27.0.2",
    "ts-pattern": "^5.9.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "^2.29.8",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.39.1",
    "@ton-ai-core/eslint-plugin-suggest-members": "^1.6.17",
    "@ton-ai-core/vibecode-linter": "^1.0.6",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.14",
    "eslint": "^9.39.1",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.5.0",
    "jscpd": "^4.0.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.14"
  }
}
